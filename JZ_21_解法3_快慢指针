class Solution {
public:
    vector<int> exchange(vector<int>& nums) {
        if(nums.size()==0 || nums.size()==1) 
            return nums;
            
        int slow=0,fast=0;
        
        while(fast->next!=NULL)
        {
            if((nums[size] & 0x1)==1)
            {
                swap(nums[slow],nums[fast]);
                slow++;
            }
            fast++;
        }
        return nums;
    }
};
